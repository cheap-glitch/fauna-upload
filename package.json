{
  "name": "fauna-upload",
  "description": "A Node module to upload schemas, resources and data to a Fauna database.",
  "version": "0.8.0",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cheap-glitch/fauna-upload.git"
  },
  "bugs": {
    "url": "https://github.com/cheap-glitch/fauna-upload/issues"
  },
  "author": "cheap glitch <cheap.glitch@gmail.com> (https://github.com/cheap-glitch)",
  "homepage": "https://github.com/cheap-glitch/fauna-upload#readme",
  "keywords": [
    "fauna",
    "faunadb",
    "fauna-database",
    "database-management",
    "database-data-upload"
  ],
  "main": "./build/index.min.js",
  "bin": {
    "fauna-upload": "build/bin/fauna-upload.js"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "npm run build:ts && npm run build:minify",
    "build:minify": "terser --compress --mangle toplevel --comments '/^!/' --output build/index.min.js -- build/index.js",
    "build:ts": "tsc-bundle ./tsconfig.json --exportAs commonjs",
    "lint": "eslint . --ext .ts --ignore-path .gitignore",
    "prepublishOnly": "npm run build",
    "test": "jest --useStderr",
    "test:install": "sudo npm uninstall -g fauna-upload && sudo npm link"
  },
  "git-hooks": {
    "pre-push": {
      "noVerify": true
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": "@cheap-glitch/typescript"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "jest-extended",
      "<rootDir>/test/setup.ts"
    ],
    "slowTestThreshold": 15,
    "coverageDirectory": ".coverage"
  },
  "dependencies": {
    "camelcase": "^6.2.0",
    "chalk": "^4.1.0",
    "cosmiconfig": "^7.0.0",
    "dotenv": "^8.2.0",
    "faunadb": "^4.0.0",
    "fetch-h2": "^2.5.1",
    "v8-compile-cache": "^2.2.0",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@cheap-glitch/eslint-config-typescript": "^1.3.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.20",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "eslint": "^7.17.0",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
